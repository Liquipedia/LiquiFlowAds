		],
		"LiquiFlowStartCode": [
			"LiquipediaAdsHooks::onLiquiFlowStartCode"
		],
		"LiquiFlowEndCode": [
			"LiquipediaAdsHooks::onLiquiFlowEndCode"
		],
		"ParserBeforeStrip": [
			"LiquipediaAdsHooks::onParserBeforeStrip"
		],
		"ParserAfterTidy": [
			"LiquipediaAdsHooks::onParserAfterTidy"
		],
		"BeforePageDisplay": [
			"LiquipediaAdsHooks::onBeforePageDisplay"
		],
		"LiquiFlowBodyFirst": [
			"LiquipediaAdsHooks::onLiquiFlowBodyFirst"
		],
		"ParserFirstCallInit": [
			"LiquipediaAdsHooks::onParserFirstCallInit"
		]
	},
	"ResourceModules": {
