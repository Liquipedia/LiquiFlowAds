{
	"name": "LiquiFlowAds",
	"version": "1.2.0",
	"author": [
		"[https://fo-nttax.de Alex Winkler]"
	],
	"url": "https://liquipedia.net/",
	"descriptionmsg": "liquiflowads-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"skin": {
			"LiquiFlow": "*"
		}
	},
	"MessagesDirs": {
		"LiquiFlowAds": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"LiquiFlowAdsHooks": "LiquiFlowAdsHooks.php"
	},
	"Hooks": {
		"LiquiFlowSidebar": [
			"LiquiFlowAdsHooks::onLiquiFlowSidebar"
		],
		"LiquiFlowTop": [
			"LiquiFlowAdsHooks::onLiquiFlowTop"
		],
		"LiquiFlowStartCode": [
			"LiquiFlowAdsHooks::onLiquiFlowStartCode"
		],
		"LiquiFlowEndCode": [
			"LiquiFlowAdsHooks::onLiquiFlowEndCode"
		],
		"ParserBeforeStrip": [
			"LiquiFlowAdsHooks::onParserBeforeStrip"
		],
		"ParserAfterTidy": [
			"LiquiFlowAdsHooks::onParserAfterTidy"
		],
		"BeforePageDisplay": [
			"LiquiFlowAdsHooks::onBeforePageDisplay"
		],
		"LiquiFlowBodyFirst": [
			"LiquiFlowAdsHooks::onLiquiFlowBodyFirst"
		],
		"ParserFirstCallInit": [
			"LiquiFlowAdsHooks::onParserFirstCallInit"
		]
	},
	"ResourceModules": {
		"ext.liquiflowads": {
			"styles": [
				"resources/css/ext.liquiflowads.css"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "LiquiFlowAds"
	},
	"manifest_version": 2
}