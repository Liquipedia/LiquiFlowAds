{
	"name": "LiquipediaAds",
	"version": "1.5.0",
	"author": [
		"[https://fo-nttax.de Alex Winkler]"
	],
	"url": "https://liquipedia.net/",
	"descriptionmsg": "liquipediaads-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"MessagesDirs": {
		"LiquipediaAds": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"Liquipedia\\Ads\\": "src/"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"Liquipedia\\Ads\\Hooks::onBeforePageDisplay"
		],
		"BruinenBodyFirst": [
			"Liquipedia\\Ads\\Hooks::onBruinenBodyFirst"
		],
		"BruinenEndCode": [
			"Liquipedia\\Ads\\Hooks::onBruinenEndCode"
		],
		"BruinenSidebar": [
			"Liquipedia\\Ads\\Hooks::onBruinenSidebar"
		],
		"BruinenStartCode": [
			"Liquipedia\\Ads\\Hooks::onBruinenStartCode"
		],
		"BruinenTop": [
			"Liquipedia\\Ads\\Hooks::onBruinenTop"
		],
		"LiquiFlowBodyFirst": [
			"Liquipedia\\Ads\\Hooks::onBruinenBodyFirst"
		],
		"LiquiFlowEndCode": [
			"Liquipedia\\Ads\\Hooks::onBruinenEndCode"
		],
		"LiquiFlowSidebar": [
			"Liquipedia\\Ads\\Hooks::onBruinenSidebar"
		],
		"LiquiFlowStartCode": [
			"Liquipedia\\Ads\\Hooks::onBruinenStartCode"
		],
		"LiquiFlowTop": [
			"Liquipedia\\Ads\\Hooks::onBruinenTop"
		],
		"ParserAfterTidy": [
			"Liquipedia\\Ads\\Hooks::onParserAfterTidy"
		],
		"ParserFirstCallInit": [
			"Liquipedia\\Ads\\Hooks::onParserFirstCallInit"
		],
		"ParserSectionCreate": [
			"Liquipedia\\Ads\\Hooks::onParserSectionCreate"
		]
	},
	"ResourceModules": {
		"ext.liquipediaads": {
			"styles": [
				"resources/styles/ext.liquipediaads.css"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "LiquipediaAds"
	},
	"config": {
		"LiquipediaAdsWhitelistedPages": {
			"value": [
				"Stream"
			],
			"path": false,
			"description": "List of special pages that are whitelisted for ads",
			"descriptionmsg": "liquipediaads-config-liquipediaadswhitelistedpages-desc",
			"public": true
		},
		"LiquipediaAdsBlacklistedPages": {
			"value": [
				"Userlogin",
				"Userlogout",
				"BotPasswords",
				"Preferences"
			],
			"path": false,
			"description": "List of special pages that are blacklisted for ads",
			"descriptionmsg": "liquipediaads-config-liquipediaadsblacklistedpages-desc",
			"public": true
		}
	},
	"manifest_version": 2
}